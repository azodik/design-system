{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "azodik-ui-docs",
  "compatibility_date": "2025-10-11",
  "assets": {
    "not_found_handling": "single-page-application"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "ui.azodik.com",
      "custom_domain": true
    }
  ]
}
